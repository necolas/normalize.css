{
  "name": "@fizzion/normalize",
  "version": "8.0.0",
  "description": "Normalize.css as a node packaged module using scss",
  "main": "_normalize.scss",
  "style": "_normalize.scss",
  "files": [
    "LICENSE.md",
    "_normalize.scss",
    "normailze/_root.scss",
    "normailze/_html5-dispaly.scss",
    "normailze/_anchors.scss",
    "normailze/_typography.scss",
    "normailze/_embedded-content.scss",
    "normailze/_grouping-content.scss",
    "normailze/_forms.scss",
    "normailze/_tables.scss"
  ],
  "devDependencies": {
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0"
  },
  "author": [
    "Nicolas Gallagher",
    "Alex Lemanski"
  ],
  "license": "MIT",
  "scripts": {
    "test": "stylelint normalize.css"
  },
  "repository": "fizzion/fizzion-normalize.scss",
  "bugs": "https://github.com/fizzion/fizzion-normalize.scss/issues",
  "homepage": "https://github.com/fizzion/fizzion-normalize.scss",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "font-family-no-duplicate-names": [
        true,
        {
          "ignoreFontFamilyNames": [
            "monospace"
          ]
        }
      ]
    }
  }
}
